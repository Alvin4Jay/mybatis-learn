<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC
        "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jay.mybatis.spring.boot.mapper.FortypeMapper">

    <insert id="insert" parameterType="com.jay.mybatis.spring.boot.bean.Fortype">
        INSERT INTO fortype(id, detail) VALUES(#{item.id,jdbcType=BIGINT}, #{item.detail,jdbcType=LONGVARCHAR})
    </insert>

</mapper>
